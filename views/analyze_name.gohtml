<script>
    document.addEventListener('DOMContentLoaded', () => {
        // ... (‡∏™‡πà‡∏ß‡∏ô
        const analyzeForm = document.getElementById('analyze-form');
        const nameInput = document.getElementById('nameInput');
        const daySelect = document.getElementById('daySelect');

        const resultMessage = document.getElementById('result-message');
        const kakisOutput = document.getElementById('kakis-output');

        const scoreSummaryContainer = document.getElementById('score-summary-container');
        const summaryPairsContainer = document.getElementById('summary-pairs-container');
        const summaryPairsOutput = document.getElementById('summary-pairs-output');

        const predictionsBox = document.getElementById('predictions-box');
        const predictionsOutput = document.getElementById('predictions-output');


        analyzeForm.addEventListener('submit', (event) => {
            event.preventDefault();
            const nameToAnalyze = nameInput.value;
            const dayToAnalyze = daySelect.value;

            // (‡∏•‡πâ‡∏≤‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏Å‡πà‡∏≤)
            resultMessage.classList.remove('error');
            resultMessage.classList.add('loading');
            resultMessage.textContent = '‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏¥‡πÄ‡∏Ñ‡∏£‡∏≤‡∏∞‡∏´‡πå...';
            kakisOutput.innerHTML = '';
            summaryPairsOutput.innerHTML = '';
            predictionsOutput.innerHTML = '';
            summaryPairsContainer.style.display = 'none';
            scoreSummaryContainer.style.display = 'none';
            predictionsBox.style.display = 'none';

            fetch('/api/v1/satnums/calculate', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                    'Accept': 'application/json'
                },
                body: JSON.stringify({
                    name: nameToAnalyze,
                    day: dayToAnalyze
                })
            })
                .then(response => {
                    if (!response.ok) {
                        return response.json().then(errData => {
                            if (errData.error === 'Day field is required') {
                                throw new Error('‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ß‡∏±‡∏ô‡πÄ‡∏Å‡∏¥‡∏î');
                            }
                            throw new Error(errData.error || '‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î‡∏à‡∏≤‡∏Å Server');
                        });
                    }
                    return response.json();
                })
                .then(data => {
                    resultMessage.textContent = '';
                    resultMessage.classList.remove('loading');

                    // 1. (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°) ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏• Kakis
                    const kakisHtml = createKakisBox(data.kakis_found);
                    kakisOutput.innerHTML = kakisHtml;

                    // 2. (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°) ‡∏£‡∏ß‡∏ö‡∏£‡∏ß‡∏°‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•
                    const allMeanings = (data.sat_num.sum_meanings || []).concat(data.sha_num.sum_meanings || []);
                    const uniqueMeanings = new Map();
                    allMeanings.forEach(meaning => {
                        if (meaning.pairnumber && !uniqueMeanings.has(meaning.pairnumber)) {
                            uniqueMeanings.set(meaning.pairnumber, meaning);
                        }
                    });

                    // 3. (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°) ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏™‡∏£‡∏∏‡∏õ‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô (‡πÉ‡∏ä‡πâ allMeanings ‡πÄ‡∏û‡∏∑‡πà‡∏≠ "‡∏ô‡∏±‡∏ö‡∏ã‡πâ‡∏≥")
                    renderOverallScore(allMeanings);
                    scoreSummaryContainer.style.display = 'block';

                    // 4. (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°) ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏™‡∏£‡∏∏‡∏õ‡∏Ñ‡∏π‡πà‡πÄ‡∏•‡∏Ç (‡πÉ‡∏ä‡πâ uniqueMeanings ‡πÄ‡∏û‡∏∑‡πà‡∏≠ "‡πÑ‡∏°‡πà‡πÅ‡∏™‡∏î‡∏á‡∏ã‡πâ‡∏≥")
                    renderSummaryPairs(uniqueMeanings);
                    summaryPairsContainer.style.display = 'block';

                    // 5. (‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°) ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏Ñ‡∏≥‡∏ó‡∏≥‡∏ô‡∏≤‡∏¢ (‡πÉ‡∏ä‡πâ uniqueMeanings ‡πÄ‡∏û‡∏∑‡πà‡∏≠ "‡πÑ‡∏°‡πà‡πÅ‡∏™‡∏î‡∏á‡∏ã‡πâ‡∏≥")
                    renderDetailedPredictions(uniqueMeanings);
                    predictionsBox.style.display = 'block';

                })
                .catch(error => {
                    // (‡∏•‡πâ‡∏≤‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡∏≠‡∏ô Error)
                    console.error('Error:', error);
                    resultMessage.classList.remove('loading');
                    resultMessage.classList.add('error');
                    resultMessage.textContent = `‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î: ${error.message}`;
                    kakisOutput.innerHTML = '';
                    summaryPairsOutput.innerHTML = '';
                    predictionsOutput.innerHTML = '';
                    summaryPairsContainer.style.display = 'none';
                    scoreSummaryContainer.style.display = 'none';
                    predictionsBox.style.display = 'none';
                });
        });

        // (‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô renderOverallScore - ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°)
        function renderOverallScore(allMeanings) {
            let goodScore = 0;
            let badScore = 0;

            allMeanings.forEach(meaning => {
                const point = meaning.pairpoint;
                if (point) {
                    if (point > 0) {
                        goodScore += point;
                    } else if (point < 0) {
                        badScore += point;
                    }
                }
            });

            document.getElementById('score-good').textContent = goodScore;
            document.getElementById('score-bad').textContent = badScore;
            document.getElementById('score-total').textContent = goodScore + badScore;
        }

        // (‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô renderSummaryPairs - ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°)
        function renderSummaryPairs(uniqueMeaningsMap) {
            let summaryHtml = '';

            uniqueMeaningsMap.forEach(meaning => {
                const pairClass = getPairTypeClass(meaning.pairtype);
                summaryHtml += `<div class="summary-pair-circle ${pairClass}">${meaning.pairnumber}</div>`;
            });

            if (summaryHtml === '') {
                summaryHtml = '<p>‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏Ñ‡∏π‡πà‡πÄ‡∏•‡∏Ç‡∏à‡∏≤‡∏Å‡∏ú‡∏•‡∏£‡∏ß‡∏°</p>';
            }
            summaryPairsOutput.innerHTML = summaryHtml;
        }

        // (‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô renderDetailedPredictions - ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°)
        function renderDetailedPredictions(uniqueMeaningsMap) {
            let meaningsHtml = '';

            if (uniqueMeaningsMap.size === 0) {
                meaningsHtml = `<p>‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏°‡∏≤‡∏¢‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ú‡∏•‡∏£‡∏ß‡∏°</p>`;
            } else {
                uniqueMeaningsMap.forEach(meaning => {
                    const pairClass = getPairTypeClass(meaning.pairtype);
                    meaningsHtml += `
                        <div class="meaning-item">
                            <div class="meaning-circle ${pairClass}">
                                ${meaning.pairnumber || '?'}
                            </div>
                            <div class="meaning-text">
                                <strong>${meaning.miracledetail || '‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠'}</strong>
                                <p>${meaning.miracledesc || '‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏Ñ‡∏≥‡∏≠‡∏ò‡∏¥‡∏ö‡∏≤‡∏¢'}</p>
                            </div>
                        </div>
                    `;
                });
            }
            predictionsOutput.innerHTML = meaningsHtml;
        }

        // (‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô getPairTypeClass - ‡πÄ‡∏´‡∏°‡∏∑‡∏≠‡∏ô‡πÄ‡∏î‡∏¥‡∏°)
        function getPairTypeClass(pairType) {
            if (!pairType) return 'pairtype-default';
            const pt = pairType.trim().toUpperCase();

            if (pt === 'D10') return 'pairtype-D10';
            if (pt === 'D8') return 'pairtype-D8';
            if (pt === 'D5') return 'pairtype-D5';

            if (pt === 'R10') return 'pairtype-R10';
            if (pt === 'R7') return 'pairtype-R7';
            if (pt === 'R5') return 'pairtype-R5';

            if (pt === 'N') return 'pairtype-N';

            if (pairType.includes('‡∏î‡∏µ')) return 'pairtype-D8';
            if (pairType.includes('‡∏£‡πâ‡∏≤‡∏¢')) return 'pairtype-R7';
            if (pairType.includes('‡∏Å‡∏•‡∏≤‡∏á')) return 'pairtype-N';

            return 'pairtype-default';
        }

