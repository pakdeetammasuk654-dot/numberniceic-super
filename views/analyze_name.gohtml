<script>
    document.addEventListener('alpine:init', () => {
        Alpine.data('analysisComponent', () => ({

            // 1. State (‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏Ç‡∏≠‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•)
            name: '',
            day: '',
            loading: false,
            error: null,
            results: null, // üëà ‡∏ô‡∏µ‡πà‡∏Ñ‡∏∑‡∏≠ JSON ‡∏ó‡∏µ‡πà‡πÑ‡∏î‡πâ‡∏à‡∏≤‡∏Å API

            // 2. Computed Properties (Getters)
            // (‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏Ñ‡πà‡∏≤‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ ‡πÄ‡∏°‡∏∑‡πà‡∏≠ 'results' ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô)

            // (‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô - ‡∏ô‡∏±‡∏ö‡∏ã‡πâ‡∏≥)
            get calculatedScores() {
                if (!this.results) return { good: 0, bad: 0, total: 0 };

                let goodScore = 0;
                let badScore = 0;

                // 2a. ‡∏£‡∏ß‡∏° Array (‡∏ô‡∏±‡∏ö‡∏ã‡πâ‡∏≥‡πÑ‡∏î‡πâ)
                const allMeanings = (this.results.sat_num.sum_meanings || [])
                    .concat(this.results.sha_num.sum_meanings || []);

                // 2b. ‡∏ß‡∏ô‡∏•‡∏π‡∏õ‡∏ô‡∏±‡∏ö‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô
                allMeanings.forEach(meaning => {
                    const point = meaning.pairpoint;
                    if (point) {
                        if (point > 0) goodScore += point;
                        else if (point < 0) badScore += point;
                    }
                });

                return {
                    good: goodScore,
                    bad: badScore,
                    total: goodScore + badScore
                };
            },

            // (‡∏´‡∏≤‡∏Ñ‡∏π‡πà‡πÄ‡∏•‡∏Ç/‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏°‡∏≤‡∏¢ ‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏ã‡πâ‡∏≥)
            get uniqueMeanings() {
                if (!this.results) return [];

                const allMeanings = (this.results.sat_num.sum_meanings || [])
                    .concat(this.results.sha_num.sum_meanings || []);

                const uniqueMap = new Map();
                allMeanings.forEach(meaning => {
                    if (meaning.pairnumber && !uniqueMap.has(meaning.pairnumber)) {
                        uniqueMap.set(meaning.pairnumber, meaning);
                    }
                });

                return Array.from(uniqueMap.values()); // üëà ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏õ‡πá‡∏ô Array
            },

            // 3. Methods (‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô)

            // (‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏¢‡∏¥‡∏á API)
            async analyzeName() {
                this.loading = true;
                this.error = null;
                this.results = null; // üëà ‡∏•‡πâ‡∏≤‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏Å‡πà‡∏≤

                try {
                    const response = await fetch('/api/v1/satnums/calculate', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                            'Accept': 'application/json'
                        },
                        body: JSON.stringify({
                            name: this.name,
                            day: this.day
                        })
                    });

                    const data = await response.json();

                    if (!response.ok) {
                        if (data.error === 'Day field is required') {
                            throw new Error('‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ß‡∏±‡∏ô‡πÄ‡∏Å‡∏¥‡∏î');
                        }
